{
	"name": "nodejs-gudsho-media-service",
	"version": "1.0.0",
	"description": "nodejs-gudsho-media-service",
	"main": "app.js",
	"private": true,
	"scripts": {
		"start:build": "nodemon --exec npm run restart",
		"restart": "rm -rf dist && npm run build && npm run serve",
		"serve": "node ./dist/server.js",
		"build": "webpack --mode development",
		"build:prod": "webpack --mode production",
		"lint": "eslint src",
		"start": "nodemon index.js",
		"precommit": "lint-staged",
		"format": "eslint src *.js --ignore-path .gitignore --fix && npm run pretty",
		"pretty": "prettier --config ./.prettierrc --w ."
	},
	"lint-staged": {
		"*.{js,json}": [
			"npm run format",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run format"
		}
	},
	"author": "GudSho",
	"license": "ISC",
	"dependencies": {
		"@babel/runtime": "^7.5.4",
		"@elastic/elasticsearch": "^7.6.0",
		"aws-sdk": "^2.541.0",
		"babel-runtime": "^6.18.0",
		"body-parser": "^1.19.0",
		"clean-webpack-plugin": "^3.0.0",
		"composable-middleware": "^0.3.0",
		"connect-multiparty": "^2.2.0",
		"cookie-parser": "^1.4.4",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-validator": "^5.0.0",
		"http": "0.0.0",
		"i18n": "^0.8.5",
		"lint-staged": "^10.0.8",
		"lodash": "^4.17.15",
		"mongoose": "^5.6.4",
		"morgan": "^1.9.1",
		"nodemon": "^1.19.4",
		"prettier": "^2.1.1",
		"redis": "^3.0.2",
		"swagger-ui-express": "^4.1.3",
		"webpack-manifest-plugin": "^2.2.0",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@babel/core": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.8.3",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.5.0",
		"@babel/preset-env": "^7.8.3",
		"@babel/register": "^7.4.4",
		"babel-core": "^6.26.3",
		"babel-loader": "^8.0.6",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-stage-0": "^6.24.1",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.0.7",
		"husky": "^4.2.3",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.10.3",
		"webpack-node-externals": "^1.7.2"
	}
}
